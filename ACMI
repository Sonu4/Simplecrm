Divisions:

SELECT scrm_divisions.id,scrm_divisions.name,scrm_divisions.deleted,scrm_divisions.assigned_user_id,scrm_divisions_scrm_services_1_c.scrm_divisions_scrm_services_1scrm_divisions_ida as division_id,scrm_divisions_scrm_services_1_c.scrm_divisions_scrm_services_1scrm_services_idb as service_id,scrm_services.name as service_name,scrm_services.deleted as service_deleted,scrm_services_aos_product_categories_1_c.scrm_services_aos_product_categories_1aos_product_categories_idb,aos_product_categories.name as service_type,aos_product_categories.id as servicetype_id,scrm_services_aos_products_1_c.scrm_services_aos_products_1scrm_services_ida,scrm_services_aos_products_1_c.scrm_services_aos_products_1aos_products_idb,aos_products.name as service_subtype FROM scrm_divisions inner join scrm_divisions_scrm_services_1_c on scrm_divisions_scrm_services_1_c.scrm_divisions_scrm_services_1scrm_divisions_ida=scrm_divisions.id
inner join scrm_services on scrm_services.id=scrm_divisions_scrm_services_1_c.scrm_divisions_scrm_services_1scrm_services_idb
inner join scrm_services_aos_product_categories_1_c on scrm_services_aos_product_categories_1_c.scrm_services_aos_product_categories_1scrm_services_ida=scrm_divisions_scrm_services_1_c.scrm_divisions_scrm_services_1scrm_services_idb
inner join aos_product_categories on aos_product_categories.id=scrm_services_aos_product_categories_1_c.scrm_services_aos_product_categories_1aos_product_categories_idb
inner join scrm_services_aos_products_1_c on scrm_services_aos_products_1_c.scrm_services_aos_products_1scrm_services_ida=scrm_divisions_scrm_services_1_c.scrm_divisions_scrm_services_1scrm_services_idb
inner join aos_products on aos_products.id=scrm_services_aos_products_1_c.scrm_services_aos_products_1aos_products_idb



Main Query:

SELECT scrm_divisions.id,scrm_divisions.name,scrm_services.name as service_name,aos_product_categories.name as service_type,aos_products.name as service_subtype FROM scrm_divisions inner join scrm_divisions_scrm_services_1_c on scrm_divisions_scrm_services_1_c.scrm_divisions_scrm_services_1scrm_divisions_ida=scrm_divisions.id
inner join scrm_services on scrm_services.id=scrm_divisions_scrm_services_1_c.scrm_divisions_scrm_services_1scrm_services_idb
inner join scrm_services_aos_product_categories_1_c on scrm_services_aos_product_categories_1_c.scrm_services_aos_product_categories_1scrm_services_ida=scrm_divisions_scrm_services_1_c.scrm_divisions_scrm_services_1scrm_services_idb
inner join aos_product_categories on aos_product_categories.id=scrm_services_aos_product_categories_1_c.scrm_services_aos_product_categories_1aos_product_categories_idb
inner join scrm_services_aos_products_1_c on scrm_services_aos_products_1_c.scrm_services_aos_products_1scrm_services_ida=scrm_divisions_scrm_services_1_c.scrm_divisions_scrm_services_1scrm_services_idb
inner join aos_products on aos_products.id=scrm_services_aos_products_1_c.scrm_services_aos_products_1aos_products_idb




select scrm_divisions.id,scrm_divisions.name,scrm_services.name as service_name,aos_product_categories.name as service_type,aos_products.name as service_subtype,aos_product_categories.name as service_2 from scrm_divisions 
inner join scrm_divisions_scrm_services_1_c on scrm_divisions_scrm_services_1_c.scrm_divisions_scrm_services_1scrm_divisions_ida=scrm_divisions.id 
inner join scrm_services on scrm_services.id=scrm_divisions_scrm_services_1_c.scrm_divisions_scrm_services_1scrm_services_idb 
inner join scrm_services_aos_product_categories_1_c on scrm_services_aos_product_categories_1_c.scrm_services_aos_product_categories_1scrm_services_ida=scrm_divisions_scrm_services_1_c.scrm_divisions_scrm_services_1scrm_services_idb 
inner join aos_product_categories on aos_product_categories.id=scrm_services_aos_product_categories_1_c.scrm_services_aos_product_categories_1aos_product_categories_idb 
inner join scrm_services_aos_products_1_c on scrm_services_aos_products_1_c.scrm_services_aos_products_1scrm_services_ida=scrm_divisions_scrm_services_1_c.scrm_divisions_scrm_services_1scrm_services_idb
inner join aos_products on aos_products.aos_product_category_id=aos_product_categories.id
where scrm_divisions.deleted=0 and aos_products.deleted=0




select scrm_divisions.id,scrm_divisions.name,scrm_services.name as service_name,aos_product_categories.name as service_type,aos_product_categories.name as service_two,aos_products.name as service_subtype from scrm_divisions 
inner join scrm_divisions_scrm_services_1_c on scrm_divisions_scrm_services_1_c.scrm_divisions_scrm_services_1scrm_divisions_ida=scrm_divisions.id 
inner join scrm_services on scrm_services.id=scrm_divisions_scrm_services_1_c.scrm_divisions_scrm_services_1scrm_services_idb 
inner join scrm_services_aos_product_categories_1_c on scrm_services_aos_product_categories_1_c.scrm_services_aos_product_categories_1scrm_services_ida=scrm_divisions_scrm_services_1_c.scrm_divisions_scrm_services_1scrm_services_idb 
inner join aos_product_categories on aos_product_categories.id=scrm_services_aos_product_categories_1_c.scrm_services_aos_product_categories_1aos_product_categories_idb 
inner join scrm_services_aos_products_1_c on scrm_services_aos_products_1_c.scrm_services_aos_products_1scrm_services_ida=scrm_divisions_scrm_services_1_c.scrm_divisions_scrm_services_1scrm_services_idb
inner join aos_products on aos_products.aos_product_category_id=aos_product_categories.id
where scrm_divisions.deleted=0 



THis Query Is for service and service type------------------------

select scrm_services.id,scrm_services.name as service_name,scrm_services_aos_product_categories_1_c.scrm_services_aos_product_categories_1aos_product_categories_idb as service_type_id,aos_product_categories.name as service_type_name,scrm_divisions.id,scrm_divisions.name from scrm_services inner join scrm_services_aos_product_categories_1_c on scrm_services_aos_product_categories_1_c.scrm_services_aos_product_categories_1scrm_services_ida=scrm_services.id inner join aos_product_categories on aos_product_categories.id=scrm_services_aos_product_categories_1_c.scrm_services_aos_product_categories_1aos_product_categories_idb
inner join scrm_divisions
where scrm_services.deleted=0 and aos_product_categories.deleted=0


